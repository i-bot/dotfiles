! ~/.Xdefaults

Xmessage.background:          #002b36
Xmessage.foreground:          #839496

! Molokai theme
URxvt*background: #001E1E
URxvt*foreground: #93a1a1


! black
*color0:#121112
*color8:#6C6C6C
! red
*color1:#D7005F
*color9:#D74083
! green
*color2:#87FF00
*color10:#93FF19
! yellow
*color3:#FF8700
*color11:#FF9926
! blue
*color4:#005F87
*color12:#387BAB
! magenta
*color5:#AF5FFF
*color13:#B973FF
! cyan
*color6:#81C1C1
*color14:#5FD7FF
! white
*color7:#AFAF87
*color15:#D0D0D0
! underline when default
*colorUL: #5FD7FF

URxvt.scrollBar: 	false
URxvt.depth: 		32
!URxvt*background: 	rgba:0000/0000/0000/9999

! Make numpad keys behave the same as the corresponding normal keys
URxvt.keysym.KP_Add:		+
URxvt.keysym.KP_Subtract:	-
URxvt.keysym.KP_Multiply:	*
URxvt.keysym.KP_Divide  :	/
URxvt.keysym.KP_Decimal :	.
URxvt.keysym.KP_Separator :	,
URxvt.keysym.KP_1:     		1
URxvt.keysym.KP_2:     		2
URxvt.keysym.KP_3:     		3
URxvt.keysym.KP_4:      	4
URxvt.keysym.KP_5:      	5
URxvt.keysym.KP_6:      	6
URxvt.keysym.KP_7:      	7
URxvt.keysym.KP_8:      	8
URxvt.keysym.KP_9:      	9
URxvt.keysym.KP_0:      	0
